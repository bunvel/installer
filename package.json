{
  "name": "@bunvel/installer",
  "version": "1.4.0",
  "type": "module",
  "description": "Cli tool to create a bunvel project",
  "keywords": [
    "Bunvel",
    "Bunvel Installer",
    "Bunvel CLI"
  ],
  "author": "Aniket Khote",
  "repository": {
    "type": "git",
    "url": "https://github.com/bunvel/installer.git"
  },
  "license": "MIT",
  "bin": {
    "bunvel": "dist/index.js"
  },
  "main": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup index.ts --format esm --dts"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    }
  },
  "devDependencies": {
    "@types/bun": "^1.2.5",
    "@types/figlet": "^1.7.0",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "@clack/prompts": "^0.10.0",
    "@inquirer/prompts": "^7.4.0",
    "chalk": "^5.4.1",
    "commander": "^13.1.0",
    "figlet": "^1.8.0"
  },
  "engines": {
    "bun": ">=1.2.0"
  }
}
